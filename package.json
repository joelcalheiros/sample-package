{
  "name": "vite",
  "version": "0.0.0",
  "author": "Joel Calheiros",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build:dev": "rollup -c ./rollup.config.dev.js",
    "build:publish": "rollup -c ./rollup.config.prod.js && npm publish",
    "build-storybook": "build-storybook",
    "test": "jest --coverage -u --maxWorkers=50% --colors --env=jsdom --json --outputFile=.jest-test-results.json || true"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@esolidar/i18n": "^0.0.209",
    "@esolidar/toolkit": "^3.6.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-jest": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-queryparams": "^6.2.9",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.7",
    "@storybook/testing-react": "^0.0.22",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.0",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "jest": "^27.1.0",
    "postcss": "^8.3.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-intl": "^5.20.10",
    "rollup-plugin-bundle-scss": "^0.1.3",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.39.0",
    "sass-loader": "^10.0.0",
    "storybook-addon-intl": "^2.4.1",
    "storybook-addon-jsx": "^7.3.13",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^1.1.1",
    "typescript": "^4.3.2",
    "vite": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./__globals__/globals.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 63
      }
    }
  }
}
